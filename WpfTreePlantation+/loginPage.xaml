<Page x:Class="WpfTreePlantation_.loginPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:WpfTreePlantation_"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="loginPage">

    <Grid>
        <Label Content="Enter login" HorizontalAlignment="Center" Height="29" Margin="289,144,312,0" VerticalAlignment="Top" Width="199"/>
        <TextBox x:Name="LOG" HorizontalAlignment="Center" Height="23" Margin="289,183,328,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="183"/>
        <Label Content="Enter password" HorizontalAlignment="Center" Margin="289,206,392,216" Width="119" RenderTransformOrigin="-1.328,-3.375" />
        <PasswordBox x:Name="PASSWORD" HorizontalAlignment="Center" Margin="289,231,328,0" VerticalAlignment="Top" Width="183" Height="23"/>
        <Button x:Name="register" Click="register_Click" Content="Sigh up" HorizontalAlignment="Left" Margin="289,272,0,0" VerticalAlignment="Top" Width="94"/>
        <Button x:Name="enter" Click="enter_Click" Content="Sigh in" HorizontalAlignment="Left" Margin="394,272,0,0" VerticalAlignment="Top" Width="94"/>
    </Grid>
</Page>
